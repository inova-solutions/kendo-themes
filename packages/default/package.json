{
  "name": "@progress/kendo-theme-default",
  "description": "SASS resources for the default Kendo UI theme",
  "version": "4.34.0",
  "author": "Progress",
  "license": "Apache-2.0",
  "keywords": [
    "Kendo UI",
    "Theme"
  ],
  "main": "dist/all.css",
  "sass": "scss/all.scss",
  "files": [
    "dist/",
    "scss/",
    "modules/"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/telerik/kendo-themes.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "buildinova": "node build/build-inova-themes.js",
    "buildinova:debug": "node --nolazy --inspect-brk=9229 build/build-inova-themes.js",
    "sass": "gulp sass",
    "sass:watch": "gulp sass:watch",
    "sass:swatches": "gulp sass:swatches",
    "sass:flat": "gulp sass:flat",
    "dart": "gulp dart",
    "dart:watch": "gulp dart:watch",
    "dart:swatches": "gulp dart:swatches",
    "dart:flat": "gulp dart:flat",
    "twbs-compat": "gulp sass --file ./build/twbs-compat.scss",
    "prepublishOnly": "echo 'no prebublish for default theme'"
  },
  "devDependencies": {
    "gulp-eslint": "^5.0.0",
    "clean-css": "^4.2.1",
    "handlebars": "^4.0.10",
    "ncp": "^2.0.0",
    "gulp": "^4.0.2",
    "@progress/kendo-theme-tasks": "^0.6.4",
    "bootstrap": "^4.5.2",
    "fs-extra": "^9.0.1",
    "tsc": "^1.20150623.0",
    "typescript": "^3.9.7"
  }
}
